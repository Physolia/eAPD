{
  "name": "cms-hitech-apd-api",
  "version": "1.0.0",
  "description": "APD api for CMS HITECH program",
  "main": "main.js",
  "scripts": {
    "audit": "npx audit-ci --config ./audit-ci.json",
    "knex-console": "node --experimental-repl-await knexrepl.js",
    "lint": "eslint '**/*.js'",
    "knex-make-migrate": "knex migrate:make",
    "knex-migrate": "knex migrate:latest",
    "knex-migrate-rollback": "knex migrate:rollback",
    "knex-migrate-down": "knex migrate:down",
    "mongoose-make-migrate": "migrate --config mongo-migrate.js create",
    "mongoose-migrate": "migrate --config mongo-migrate.js --autosync up",
    "mongoose-migrate-down": "migrate --config mongo-migrate.js down",
    "migrate": "yarn run knex-migrate && yarn run mongoose-migrate",
    "seed": "knex seed:run --specific=main.js",
    "seed-dev": "NODE_ENV=development DEV_DB_HOST=localhost yarn run seed",
    "seed-users": "knex seed:run --specific=base-users.js",
    "start": "node main.js",
    "start-dev": "NODE_ENV=development nodemon --inspect=0.0.0.0 ./main.js -e js",
    "jest": "NODE_ENV=test jest",
    "tap": "NODE_ENV=test tap -J --no-browser --no-coverage --no-timeout --reporter=spec ${TESTS:-'{,!(node_modules)/**/}*.test.js'}",
    "test": "NODE_ENV=test tap -J --cov --coverage-report=lcov --lines=75 --functions=60 --branches=50 --statements=75  --no-browser --no-timeout --reporter=spec --test-arg=--silent '{,!(node_modules)/**/}*.test.js'",
    "test-endpoints": "jest -u --detectOpenHandles 'endpoint'",
    "test-specific": "NODE_ENV=test tap --cov --coverage-report=lcov --no-browser --reporter=spec --lines=0 --functions=0 --branches=0 --statements=0",
    "prettier": "prettier --config \"../.prettierrc\" --write \"**/*.js\"",
    "fullTest": "./unit-test.sh",
    "npm-check": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/eAPD.git"
  },
  "keywords": [
    "cms",
    "medicaid",
    "hitech",
    "emr",
    "apd"
  ],
  "contributors": [
    "Nicolas Aretakis",
    "Akua Amponsah",
    "Heather Battaglia",
    "Ty Bolt",
    "Ronald Bronson",
    "Bill Brooks",
    "Richard Davis",
    "Jenn Downs",
    "Nicole Fenton",
    "Tiffany Forkner",
    "Eghosa Guobadia",
    "Austin Hernandez",
    "James Hupp",
    "Jesse James",
    "Meghana Khandekar",
    "Michael Knoll",
    "Jerome Lee",
    "Nikki Lee",
    "Christopher (CM) Lubinksi",
    "Chris Miraflor",
    "Darren Mirano",
    "Aviva Oskow",
    "Paul Phillips",
    "Laura Ponc√©",
    "Brendan Sudol",
    "Caryn Swartz",
    "Jen Thibault",
    "Greg Walker",
    "Nichole Weems",
    "Nicole Zonnenberg"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/eAPD/issues"
  },
  "homepage": "https://github.com/CMSgov/eAPD#readme",
  "engines": {
    "node": "16.13.2"
  },
  "dependencies": {
    "@okta/jwt-verifier": "2.3.0",
    "@okta/okta-sdk-nodejs": "6.4.0",
    "ajv": "8.10.0",
    "aws-sdk": "2.1090.0",
    "axios": "0.26.1",
    "body-parser": "1.19.2",
    "chalk": "4.1.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "dotenv": "15.0.0",
    "express": "4.17.3",
    "fast-json-patch": "3.1.0",
    "file-type": "16.5.3",
    "form-data": "4.0.0",
    "jsonpatch": "3.1.0",
    "jsonpatch-to-mongodb": "1.0.0",
    "jsonpointer": "5.0.0",
    "jsonwebtoken": "8.5.1",
    "knex": "1.0.3",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "5.13.14",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "pg": "8.7.3",
    "sanitize-html": "2.7.0",
    "sharp": "0.30.2",
    "swagger-ui-express": "4.3.0",
    "uuid": "8.3.2",
    "winston": "3.6.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "colors": "1.4.0",
    "eslint": "8.10.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "sinon": "13.0.1",
    "supertest": "6.2.2",
    "tap": "15.1.6"
  },
  "updateRestrictions": {
    "file-type": "can't upgrade to 17 because it requires babel",
    "mongoose": "we aren't upgrading to 6.x yet because it's not highly supported by other packages"
  },
  "jest": {
    "globalSetup": "./jest.setup.js",
    "globalTeardown": "./jest.teardown.js",
    "testMatch": [
      "**/?(*.)+(spec|test|endpoint).js?(x)"
    ],
    "testEnvironment": "node",
    "testTimeout": 30000
  }
}
